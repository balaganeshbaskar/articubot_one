<sdf version="1.6">
  <world name="default">
    <!-- Enable ROS spawn services -->
    <plugin name="gazebo_ros_factory" filename="libgazebo_ros_factory.so"/>

    <!-- Optionally add ground plane, sun, etc. -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <include>
      <uri>model://sun</uri>
    </include>
  </world>
</sdf>
